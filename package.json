{
  "name": "n8n-nodes-clicksign",
  "version": "1.0.0",
  "description": "Clicksign API integration for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Engenharia Clicksign",
    "email": "engenharia@clicksign.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/clicksign/n8n-nodes-clicksign.git"
  },
  "engines": {
    "node": ">=20.15"
  },
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "pnpm dlx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier *.{ts,js,json} --write",
    "lint": "eslint *.{ts,js,json}",
    "lintfix": "eslint *.{ts,js,json} --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c eslint.prepublish.config.js nodes credentials package.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/ClicksignApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Clicksign/Clicksign.node.js"
    ]
  },
  "lint-staged": {
    "*.{ts,js,json}": [
      "pnpm format",
      "pnpm lintfix"
    ]
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^9.31.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-mock-extended": "^4.0.0",
    "lint-staged": "^16.1.2",
    "picomatch": "^4.0.2",
    "prettier": "^3.5.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
